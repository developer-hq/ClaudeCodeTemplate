{
  "qwen": {
    "cli_path": "qwen-code",
    "api_endpoint": null,
    "api_key": null,
    "timeout": 300,
    
    "enable_for": [
      "batch_operations",
      "documentation_generation",
      "large_codebase_analysis", 
      "simple_refactoring",
      "translation"
    ],
    
    "thresholds": {
      "large_task_tokens": 20000,
      "max_context_files": 10
    },
    
    "health_check": {
      "interval": 60,
      "retry_count": 3
    },
    
    "logging": {
      "level": "INFO",
      "file": ".claude/qwen.log"
    }
  },
  
  "task_routing": {
    "patterns": {
      "batch_format": {
        "keywords": ["format", "formatting", "格式化"],
        "min_tokens": 20000
      },
      "documentation": {
        "keywords": ["docs", "document", "文档", "generate docs", "create documentation"]
      },
      "translation": {
        "keywords": ["translate", "翻译", "trans", "translation"]
      },
      "refactoring": {
        "keywords": ["rename", "refactor", "重构", "restructure"]
      },
      "analysis": {
        "keywords": ["analyze", "analysis", "分析", "find pattern", "search"],
        "min_files": 10
      }
    }
  },
  
  "fallback": {
    "always_use_claude_for": [
      "architecture design",
      "complex debugging", 
      "algorithm optimization",
      "performance tuning"
    ]
  }
}